{
  "name": "@sidekit/mcp",
  "type": "module",
  "version": "0.0.0-alpha.1",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kerwanp/sidekit.git"
  },
  "bugs": {
    "url": "https://github.com/kerwanp/sidekit/issues"
  },
  "homepage": "https://github.com/kerwanp/sidekit",
  "files": [
    "build",
    "!build/tests",
    "!build/bin/schemas*",
    "!build/bin/registry*",
    "!build/bin/test*"
  ],
  "scripts": {
    "build": "tsc",
    "lint": "eslint .",
    "quick:test": "node --import=./tsnode.esm.js --enable-source-maps bin/test.ts",
    "test": "c8 yarn quick:test",
    "typecheck": "tsc --noEmit",
    "gen:schemas": "node --import=./tsnode.esm.js --enable-source-maps bin/schemas.ts",
    "gen:registry": "node --import=./tsnode.esm.js --enable-source-maps bin/registry.ts",
    "dev": "tsc --watch"
  },
  "bin": "./build/bin/sidekit.js",
  "exports": {
    ".": "./build/src/index.js",
    "./types": "./build/src/types.js"
  },
  "devDependencies": {
    "@adonisjs/eslint-config": "^2.1.2",
    "@adonisjs/tsconfig": "^1.4.1",
    "@japa/assert": "^4.1.1",
    "@japa/expect": "^3.0.6",
    "@japa/expect-type": "^2.0.3",
    "@japa/file-system": "^2.3.2",
    "@japa/runner": "^4.4.0",
    "@swc/core": "^1.13.3",
    "@types/better-sqlite3": "^7.6.13",
    "@types/node": "^24.2.1",
    "c8": "^10.1.3",
    "eslint": "^9.33.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.2"
  },
  "packageManager": "yarn@4.9.2",
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.17.3",
    "@sidekit/search": "workspace:*",
    "@xenova/transformers": "^2.17.2",
    "better-sqlite3": "^12.2.0",
    "pathe": "^2.0.3",
    "sqlite-vec": "^0.1.7-alpha.2",
    "zod": "3"
  },
  "keywords": [
    "cli",
    "ai",
    "cursor",
    "opencode",
    "claude",
    "copilot"
  ],
  "publishConfig": {
    "access": "public",
    "tag": "latest"
  }
}
